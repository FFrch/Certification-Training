<%= simple_form_for [ @tree ] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_year %>
  <%= f.input :fruits, collection: ["Apple", "Apricot", "Cherry", "Chestnut", "Coconut tree", "Fig", "Grapefruit", "Hazel", "Mandarin", "Mango", "Mirabellier", "Mulberry", "Olivier", "Orange", "Pear", "Plum"] %>
  <%= f.input :quantity_by_year %>
  <%= f.submit "Add this tree", class: "btn btn-primary" %>
<% end %>
